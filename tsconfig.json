{
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "moduleResolution": "node",
    "importHelpers": true,
    "isolatedModules": false,
    "noEmit": true,
    "strict": true,
    "allowSyntheticDefaultImports": true,
    "suppressImplicitAnyIndexErrors": true,
    "noImplicitAny": false,
    "strictNullChecks": false
  },
  "exclude": ["node_modules", "node_modules/element-plus", "**/*.spec.ts"]
}
